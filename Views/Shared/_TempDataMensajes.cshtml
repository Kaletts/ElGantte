@if (TempData["Success"] != null)
{
    <div class="alert alert-success shadow-sm bg-body-tertiary rounded">@TempData["Success"]</div>
}
@if (TempData["Error"] != null)
{
    <div class="alert alert-danger shadow-sm bg-body-tertiary rounded">@TempData["Error"]</div>
}
@if (TempData["Warning"] != null)
{
    <div class="alert alert-warning shadow-sm bg-body-tertiary rounded">@TempData["Warning"]</div>
}
